<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <padding
                android:bottom="15dp"
                android:left="15dp"
                android:right="15dp"
                android:top="15dp"/>
            <stroke
                android:width="1.5dp"
                android:color="#abb0ca"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <size
                android:width="120dp"
                android:height="120dp"/>
            <stroke
                android:width="1.5dp"
                android:color="#abb0ca"/>
        </shape>

    </item>
</layer-list>